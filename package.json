        "inversify",
        "library",
        "example"
    ],
    "author": "pavan palakshamurthy",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/pavanpalakshamurthy/ts-inversify-lib/issues"
    },
    "homepage": "https://github.com/pavanpalakshamurthy/ts-inversify-lib#readme",
    "devDependencies": {
        "inversify": "^5.0.1",
        "ts-loader": "^9.4.2",
        "typescript": "^3.4.5",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1"
    },
    "dependencies": {
        "dts-generator": "^3.0.0"
